{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "yoga-landing-page/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/send-guide",
      "dest": "server/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "yoga-landing-page/dist/$1"
    }
  ],
  "outputDirectory": "yoga-landing-page/dist"
}
